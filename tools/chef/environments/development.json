{
  "name": "development",
  "description": "",
  "json_class": "Chef::Environment",
  "chef_type": "environment",

  "default_attributes": {
    "apache": {
      "sites": {
        "lorekeepers.org": {
          "inherits": "my-app",
          "server_name": "lorekeepers.org.dev",
          "docroot": "/vagrant/public",
          "capistrano": false
        }
      }
    },
    "mysql": {
      "users": {
        "lorekeep_eqdkp": {
          "database_name": "lorekeep_eqdkp"
        },
        "lorekeep_forum": {
          "database_name": "lorekeep_forum"
        }
      }
    }
  },

  "override_attributes": {
    "capistrano": {
      "group": false
    },
    "data-bag-merge": {
      "environments": {
        "encrypted": false
      }
    }
  }
}
